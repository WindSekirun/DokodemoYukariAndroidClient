            android:layout_gravity="bottom"
            app:itemBackground="@drawable/drawer_background"
            app:itemIconTint="@drawable/drawer_tint"
            app:itemTextColor="@drawable/drawer_tint"
            app:layout_constraintEnd_toEndOf="parent"
